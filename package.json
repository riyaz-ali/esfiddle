  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "1.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^3.2.4",
    "istanbul": "0.4.5",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0",
    "vue": "^2.4.2"
  },
  "scripts": {
    "test": "npm run lint && mocha test/*.test.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext js,vue src/** server/** test/**",
    "build": "rimraf dist && npm run build:style && npm run build:editor",
    "build:style": "lessc style/profile.less dist/style/profile.css && lessc style/blog.less dist/style/blog.css && lessc style/about.less dist/style/about.css",
    "build:editor": "cross-env NODE_ENV=production webpack --config build/webpack.editor.js --progress --hide-modules",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "rimraf dist && npm run build:style && webpack-dev-server --config build/webpack.editor.js --hot --open",
    "dev:server": "node server/app.js"
  },
  "engines": {
    "node": "^6.11.1 || ^7.10.1 || >=8.1.4"
  }
}
